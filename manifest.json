{
  "name": "ENT Lemans Auto Inject",
  "description": "Inject Your Login, Password and Recherche in ENT Lemans",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://cas.univ-lemans.fr/cas/login*",
        "http://planning.univ-lemans.fr/*"
      ],
      "js": ["app.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "ext/index.html"
  },
  "permissions": ["storage", "tabs", "activeTab", "declarativeContent"]
}
